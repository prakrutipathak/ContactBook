@model Contact
<h1>Details Contact</h1>
<div>
   
    <dl class="row">

        <dt class="col-sm-2">
            First Name
        </dt>
        <dd class="col-sm-10">
            @Model.FirstName
        </dd>
        <dt class="col-sm-2">
            Last Name
        </dt>
        <dd class="col-sm-10">
            @Model.LastName
        </dd>

        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
        <dt class="col-sm-2">
            Address
        </dt>
        <dd class="col-sm-10">
            @Model.Address
        </dd>

    </dl>
    <p>
        @if (User.Identity.IsAuthenticated)
{
        <a asp-action="Edit" asp-route-id="@Model.ContactId">Edit</a>
        <a asp-action="Delete" asp-route-id="@Model.ContactId">Delete</a>
}

    </p>
</div>